<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Elo.mainMenuController">
   <children>

   </children>
   <top>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="44.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" />
   </top>
   <left>
      <VBox prefHeight="273.0" prefWidth="82.0" />
   </left>
   <center>
      <HBox prefHeight="323.0" prefWidth="436.0">
         <children>
            <VBox alignment="CENTER">
               <children>
                  <TableView fx:id="tableView" maxHeight="1.7976931348623157E308">
                     <columns>
                        <TableColumn fx:id="filmId" text="Film Title">
                        </TableColumn>
                        <TableColumn fx:id="yearId" prefWidth="44.0" text="Year">
                        </TableColumn>
                        <TableColumn fx:id="eloId" prefWidth="76.0" text="Elo Rating">
                        </TableColumn>
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <Button fx:id="btnMainMenu" mnemonicParsing="false" onAction="#OpenMainMenu" text="Add new Film" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="btnMovieList" mnemonicParsing="false" onAction="#OpenMovieList" text="MovieList" />
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" />
                  <Button fx:id="btnUserMain" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#OpenUserMainView" text="UserMainView" />
               </children>
            </VBox>
         </children></HBox>

   </center>
   <right>
      <VBox prefHeight="273.0" prefWidth="82.0" />
   </right>
   <bottom>
      <Pane prefHeight="128.0" prefWidth="600.0" BorderPane.alignment="CENTER" />
   </bottom>

</BorderPane>
